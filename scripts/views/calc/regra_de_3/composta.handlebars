<div id="main_container">
    <div class="data-container">
        <form id="data_box_form" class="data-box calc-data-box-form">
            <section class="header">
                <span>Regra de 3</span>
                <div id="calc_option">
                    <ul>
                        <li><button type="button" link="/regra-de-3/simples">Simples</button></li>
                        <li><button type="button" class="on" link="/regra-de-3/composta">Composta</button></li>
                    </ul>'
                    <div id="calc_option_button_left" class="button"><img class="arrow-thin" src="/images/icons/arrow/arrow_thin_dark.svg" alt=""></div>
                    <div id="calc_option_button_right" class="button"><img class="arrow-thin" src="/images/icons/arrow/arrow_thin_dark.svg" alt=""></div>
                </div>
            </section>
            <div id="data_box_form_input">
                <section class="calc-multiple-input-container">
                    <div class="label-float">
                        <input type="number" step="any" id="valor_1" class="calc-input" autocomplete="off" placeholder=" " required>    
                        <label>Valor 1 </label>
                    </div>
                    <span class="or-input-separation">-</span>
                    <div class="label-float">
                        <input type="number" step="any" id="valor_2" class="calc-input" autocomplete="off" placeholder=" " required>    
                        <label>Valor 2 </label>
                    </div>
                </section>
                <section class="calc-multiple-input-container">
                    <div class="label-float">
                        <input type="number" step="any" id="valor_3" class="calc-input" autocomplete="off" placeholder=" " required>    
                        <label>Valor 3 </label>
                    </div>
                    <span class="or-input-separation">-</span>
                    <div class="label-float">
                        <input type="number" step="any" id="valor_4" class="calc-input" autocomplete="off" placeholder=" " required>    
                        <label>Valor 4 </label>
                    </div>
                </section>
                <section class="calc-multiple-input-container">
                    <div class="label-float">
                        <input type="number" step="any" id="valor_5" class="calc-input" autocomplete="off" placeholder=" ">    
                        <label>Valor 5 (opcional) </label>
                    </div>
                    <span class="or-input-separation">-</span>
                    <div class="label-float">
                        <input type="number" step="any" id="valor_6" class="calc-input" autocomplete="off" placeholder=" ">    
                        <label>Valor 6 (opcional) </label>
                    </div>
                </section>
                <section class="calc-multiple-input-container">
                    <div class="label-float">
                        <input type="number" step="any" id="valor_7" class="calc-input" autocomplete="off" placeholder=" " required>    
                        <label>Valor 7 </label>
                    </div>
                    <span class="or-input-separation">-</span>
                    <div class="label-float">
                        <input type="number" step="any" id="valor_8" class="calc-input" autocomplete="off" style="pointer-events: none; border-bottom: solid 2px black;" placeholder=" " disabled>    
                        <label>X</label>
                    </div>
                </section>
            </div>
            <button type="submit" id="calc_button_calculate" form="data_box_form">Calcular</button>
        </form>
        <div id="data_box_result" class="data-box">
            <section class="header">
                <span>Resultado</span>
            </section>
            <span id="zzz">zzz</span>
            <div id="calc_result_content_container">
                <div id="calc_result_text_box">
                    <span id="calc_result_text"></span>
                </div>
            </div>
            <img copy-element="calc_result_text" class="clipboard-icon" src="/images/icons/clipboard/clipboard.svg">
        </div>
    </div>
</div>